{
    "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "target": "es2016",
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": false
    }
}
